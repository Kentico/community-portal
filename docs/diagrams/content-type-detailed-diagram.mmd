        AE[AutoresponderEmail]
        NE[NewsletterEmail]
    end
    
    subgraph ReusableContentTypes[Reusable Content Types]
        AC[AuthorContent]
        BPC[BlogPostContent]
        IC[ImageContent]
        LC[LinkContent]
        EDC[EmailDefaultsContent]
    end
    
    subgraph WebPageContentTypes[Web Page Content Types]
