{
  "name": "kentico-community-portal-web-admin",
  "version": "1.0.0",
  "description": "Xperience by Kentico Community Portal administration project.",
  "private": true,
  "scripts": {
    "start": "npm run lint && webpack serve --mode development",
    "build": "npm run lint && webpack --mode production",
    "lint": "npx eslint src/**/*.tsx"
  },
  "engines": {
    "node": ">=18.12.0 <=19"
  },
  "dependencies": {
    "@kentico/xperience-admin-base": "28.0.2",
    "@kentico/xperience-admin-components": "28.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.5",
    "@babel/plugin-transform-runtime": "7.23.4",
    "@babel/preset-env": "7.23.5",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@kentico/xperience-webpack-config": "28.0.2",
    "@types/react": "18.2.41",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "babel-loader": "9.1.3",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "40.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "3.1.0",
    "typescript": "5.3.2",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react"
  },
  "browserslist": [
    "> .2% and last 2 versions",
    "not op_mini all",
    "not IE 11"
  ]
}
