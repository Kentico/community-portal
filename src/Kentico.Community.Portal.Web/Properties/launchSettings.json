      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_WATCH_RESTART_ON_RUDE_EDIT": "true"
      }
    },
    "Portal.WebCI": {
      "commandName": "Project",
      "launchBrowser": false,
      "applicationUrl": "https://localhost:45039",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "CI"
      }
    }
  }
