@using Kentico.Community.Portal.Web.Features.LandingPages

@model TemplateViewModel<LandingPageDefaultTemplateProperties>

@{
    var templateModel = Model.GetTemplateModel<LandingPage>();
}

<vc:page-heading page="new(templateModel)" />

<main>
    <editable-area area-identifier="top" allow-widget-output-cache="View.CacheEnabled"
        widget-output-cache-expires-after="TimeSpan.FromMinutes(5)" />
</main>
