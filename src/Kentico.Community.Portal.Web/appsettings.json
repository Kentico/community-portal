{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "XperienceEventLog": {
      "LogLevel": {
        "Default": "Error",
        "Kentico.Community.Portal": "Warning",
        "Kentico.Community.Portal.Web.Features.Members.Badges": "Information",
        "Kentico.Community.Portal.Web.Features.QAndA.Notifications": "Information",
        "Microsoft.AspNetCore.Server.Kestrel": "None",
        "Microsoft.AspNetCore.Antiforgery": "Critical"
      }
    }
  },
  "AllowedHosts": "*",

  "CMSProcessContactActionsInterval": 30,

  "CMSHeadless": {
    "AllowIntrospection": false,
    "EnableTracking": false,
    "GraphQlEndpointPath": "/api/headless",
    "CorsAllowedOrigins": "*",
    "CorsAllowedHeaders": "Authorization;Cache-Control;Content-Type",
    "Caching": {
      "AbsoluteExpiration": "360"
    }
  },

  "CMSBuilderScriptsIncludeJQuery": false,

  "CMSLuceneSearch": {
    "PostStartupReindexingOptions": {
      "Enabled": true,
      "IndexesExcludedFromAutomaticReindexing": [],
      "CheckIntervalMinutes": 2
    }
  },

  "SupportRequestProcessing": {
    "IsEnabled": true
  },

  "Vite": {
    "Base": "dist/",
    "Manifest": "manifest.json"
  }
}
