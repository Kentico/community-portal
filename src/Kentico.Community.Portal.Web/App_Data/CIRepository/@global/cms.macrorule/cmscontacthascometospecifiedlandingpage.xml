<?xml version="1.0" encoding="utf-8"?>
<cms.macrorule>
  <MacroRuleCondition>
    <![CDATA[{_perfectum}Contact.CameToLandingPage("{page}")]]>
  </MacroRuleCondition>
  <MacroRuleDescription>
    <![CDATA[{$macrorule.cmscontacthascometospecifiedlandingpage.description$}]]>
  </MacroRuleDescription>
  <MacroRuleDisplayName>Contact arrived on landing page</MacroRuleDisplayName>
  <MacroRuleEnabled>True</MacroRuleEnabled>
  <MacroRuleGUID>78f662de-147b-4835-857c-cf28f3292795</MacroRuleGUID>
  <MacroRuleIsCustom>False</MacroRuleIsCustom>
  <MacroRuleName>CMSContactHasComeToSpecifiedLandingPage</MacroRuleName>
  <MacroRuleParameters>
    <form>
      <field column="page" columnprecision="0" columntype="pages" guid="73e30082-df39-4e0c-bda1-93cd7cc03b5d" visible="true">
        <properties>
          <fieldcaption>select page</fieldcaption>
        </properties>
        <settings>
          <controlname>Kentico.Administration.WebPageSelector</controlname>
          <MaximumPages>1</MaximumPages>
          <Sortable>False</Sortable>
        </settings>
      </field>
      <field column="_perfectum" columnsize="100" columntype="text" guid="f5cc5af0-9cff-4836-9522-ce122a856345" visible="true">
        <properties>
          <defaultvalue>__is</defaultvalue>
          <fieldcaption>select operation</fieldcaption>
        </properties>
        <settings>
          <controlname>Kentico.Administration.DropDownSelector</controlname>
          <IsPerfectum>True</IsPerfectum>
          <Options>
<![CDATA[
__is;has
__isnot;has not
]]>
          </Options>
          <RepeatDirection>horizontal</RepeatDirection>
        </settings>
      </field>
    </form>
  </MacroRuleParameters>
  <MacroRuleText>Contact {_perfectum} come to landing page {page}</MacroRuleText>
</cms.macrorule>